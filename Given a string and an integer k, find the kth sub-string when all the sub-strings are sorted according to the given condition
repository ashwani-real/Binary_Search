#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define V vector    
#define pb push_back     
#define mp make_pair      
#define p pair           
#define f first            
#define s second           
#define Q queue               
#define S stack               
#define test ll t;cin>>t;while(t--)
#define md 1e9+7
#define all(x) x.begin(),x.end()
#define fast ios_base::sync_with_stdio(false),cin.tie(NULL),cout.tie(NULL)
#define endl "\n"
ll i,j,k,n,n1,n2,n3,n4,m,a,b,c,d,x,y,z,cnt=0,cnt1=0,flag=0,flag1=0,sum=0,sum1=0,ans=0,ans1=0,mn=INT_MAX,mx=INT_MIN;
void ashwani(){
	#ifndef ONLINE_JUDGE
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
	freopen("error.txt","w",stderr);
	#endif
}
int main(){
	ashwani();fast;
	string s1;
	cin>>s1;
	cin>>k;
	n=s1.length();
	ll psum[n]={0};
	psum[0]=n;
	for(i=1;i<n;i++){
		psum[i]=psum[i-1]+n-i;
	}
	
	ll l=0,r=n-1,mid,ans=-1;
	while(l<=r){
		mid=l+(r-l)/2;
		if(psum[mid]>=k){
			ans=mid;
			r=mid-1;
		}
		else
		l=mid+1;
	}
	if(ans==0){
		for(i=0;i<k;i++)
			cout<<s1[i];
		cout<<endl;
	}
	else{
		ll end=k-(psum[ans-1]);
		for(i=0;i<end;i++)
			cout<<s1[ans+i];
		cout<<endl;
	}
	return 0;
}
